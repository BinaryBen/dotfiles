        "button": {
          "text": "$(tools) Powertools",
          "isRight": true,
          "tooltip": "View all available Powertools"
        }
      },
      "Download extensions": {
        "description": "Download VSIX files that aren't on the Open VSX Registry",
        "script": "commands/downloadVSIX.js"
      }
    }
