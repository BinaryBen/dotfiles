{
  "codeswing.layout": "splitLeft",
  "cSpell.language": "en-GB",
  "editor.accessibilitySupport": "off",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "Input NF, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 13,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.showSlider": "always",
  "editor.tabSize": 2,
  "editor.unicodeHighlight.allowedCharacters": {
    "Ô∏è": true
  },
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 120,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.closeExtensionDetailsOnViewChange": true,
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "git.autofetch": true,
  "git.confirmSync": false,
  // #if(editor="visual studio code")
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "markdown": false
  },
  // #endif
  "gitlens.currentLine.format": "${author, }${agoOrDate}",
  "gitlens.plusFeatures.enabled": false,
  "gitlens.showWelcomeOnInstall": false,
  "keyboard.touchbar.enabled": false,
  "peacock.affectStatusBar": false,
  "peacock.affectTitleBar": false,
  "peacock.favoriteColors": [
    {
      "name": "Angular Red",
      "value": "#dd0531"
    },
    {
      "name": "Azure Blue",
      "value": "#007fff"
    },
    {
      "name": "JavaScript Yellow",
      "value": "#f9e64f"
    },
    {
      "name": "Mandalorian Blue",
      "value": "#1857a4"
    },
    {
      "name": "Node Green",
      "value": "#215732"
    },
    {
      "name": "React Blue",
      "value": "#61dafb"
    },
    {
      "name": "Something Different",
      "value": "#832561"
    },
    {
      "name": "Svelte Orange",
      "value": "#ff3d00"
    },
    {
      "name": "Vue Green",
      "value": "#42b883"
    }
  ],
  "prettier.requireConfig": true,
  "redhat.telemetry.enabled": false,
  "telemetry.telemetryLevel": "off",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.fontFamily": "Input NF",
  "terminal.integrated.fontSize": 13,
  "theme-by-language.themes": {
    "*": "Default Dark+",
    "csv": "Office Theme (Excel)"
  },
  // Prefer to only know about new features when they are also available in Codium
  // #if(editor="visual studio code")
  "update.showReleaseNotes": false,
  // #endif
  "vscode-edge-devtools.mirrorEdits": true,
  "window.commandCenter": true,
  "window.restoreWindows": "none",
  "workbench.editorAssociations": {
    "*.csv": "gc-excelviewer-csv-editor",
    "*.xlsx": "gc-excelviewer-csv-editor"
  },
  "workbench.iconTheme": "vscode-great-icons",
  "workbench.startupEditor": "none",
  "zenMode.fullScreen": false
}