{
  "advancedNewFile.exclude": {
    "node_modules": true,
    "node_modules_electron": true,
    "dist": true,
    "out": true,
    "build": true
  },
  "advancedNewFile.showInformationMessages": true,
  "advancedNewFile.convenienceOptions": ["last", "current", "root"],
  "codeswing.layout": "splitLeft",
  "cSpell.language": "en-GB","customizeUI.activityBar": "bottom",
  "customizeUI.activityBarHideSettings": true,
  "customizeUI.font.regular": "Helvetica Neue",
  "customizeUI.statusBarPosition": "under-panel",
  "customizeUI.stylesheet": {
    ".monaco-pane-view .monaco-split-view2.vertical>.monaco-scrollable-element.mac": "height: calc(100% - 90px) !important;",
    ".monaco-split-view2.vertical>.monaco-scrollable-element.mac>.split-view-container>.split-view-view.visible>.part.sidebar.left>.content": "height: 100vh !important",
    "div.monaco-grid-view > div > div > div.monaco-scrollable-element.mac > div.split-view-container > div.split-view-view.visible > div > div > div.monaco-scrollable-element.mac > div.split-view-container > div:nth-child(1) > div > div > div.monaco-scrollable-element.mac > div.split-view-container > div:nth-child(2)": "bottom: 0px !important; top: unset !important; height: 48px !important",
    ".monaco-pane-view .monaco-scrollable-element.mac > div.split-view-container > div:not(:first-child)": "transform: translateY(-20px) !important",
    ".part.activitybar.left": "background-color: unset !important",
    "body.activity-bar-at-bottom .monaco-workbench .activitybar > .content .composite-bar": "margin-left: 0 !important",
    "body.inline-title-bar .part>.title>.title-actions .action-label": "min-width: unset !important",
    ".monaco-workbench .part.sidebar>.title>.title-label h2": "display: none;",
    ".part.activitybar.left.bordered": "border: 1px solid transparent!important",
    ".activitybar .composite-bar > .monaco-action-bar": "padding: 0 10px 0 6px",
    ".activitybar .composite-bar > .monaco-action-bar > .actions-container": "justify-content: space-evenly; max-width: 300px; border: 1px solid #ffffff1c; border-radius: 6px",
    ".pane-header": "padding-left: 10px;",
    "body.activity-bar-at-bottom .monaco-workbench .activitybar > .content": "flex-direction: column !important"
  },
  "customizeUI.titleBar": "inline",
  "editor.accessibilitySupport": "off",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "Input NF, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 13,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.inlineSuggest.enabled": true,
  "editor.insertSpaces": true,
  "editor.minimap.showSlider": "always",
  "editor.rulers": [
    {
      "column": 80,
      "color": "#312e2e"
    },
    {
      "column": 120,
      "color": "#3f0101"
    }
  ],
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.tabSize": 2,
  "editor.unicodeHighlight.allowedCharacters": {
    "Ô∏è": true
  },
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 120,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.closeExtensionDetailsOnViewChange": true,
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "files.associations": {
    "*.module": "php"
  },
  "files.autoSave": "onFocusChange",
  "git.autofetch": true,
  "git.confirmSync": false,
  // #if(editor="visual studio code")
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "markdown": false
  },
  // #endif
  "gitlens.currentLine.format": "${author, }${agoOrDate}",
  "gitlens.plusFeatures.enabled": false,
  "gitlens.showWelcomeOnInstall": false,
  "keyboard.touchbar.enabled": false,
  "peacock.affectActivityBar": false,
  "peacock.affectStatusBar": true,
  "peacock.affectTitleBar": false,
  "peacock.favoriteColors": [
    {
      "name": "Angular Red",
      "value": "#dd0531"
    },
    {
      "name": "Azure Blue",
      "value": "#007fff"
    },
    {
      "name": "JavaScript Yellow",
      "value": "#f9e64f"
    },
    {
      "name": "Mandalorian Blue",
      "value": "#1857a4"
    },
    {
      "name": "Node Green",
      "value": "#215732"
    },
    {
      "name": "React Blue",
      "value": "#61dafb"
    },
    {
      "name": "Something Different",
      "value": "#832561"
    },
    {
      "name": "Svelte Orange",
      "value": "#ff3d00"
    },
    {
      "name": "Vue Green",
      "value": "#42b883"
    }
  ],
  "peacock.showColorInStatusBar": false,
  "peacock.surpriseMeOnStartup": true,
  "prettier.requireConfig": true,
  "redhat.telemetry.enabled": false,
  "sidebar-markdown-notes.leftMargin": true,
  "syncSettings.keybindingsPerPlatform": false,
  "syncSettings.resources": ["extensions", "keybindings", "settings"],
  "telemetry.telemetryLevel": "off",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.fontFamily": "Input NF",
  "terminal.integrated.fontSize": 13,
  "theme-by-language.themes": {
    "*": "Default Dark+",
    "csv": "Office Theme (Excel)"
  },
  // * Prefer to only know about new features when they are also available in Codium
  // #if(editor="vscodium")
  "update.showReleaseNotes": false,
  // #endif
  "vscode-edge-devtools.mirrorEdits": true,
  "window.commandCenter": true,
  "window.restoreWindows": "one",
  "window.titleBarStyle": "native",
  "workbench.editor.closeEmptyGroups": false,
  "workbench.editor.showTabs": false,
  "workbench.editorAssociations": {
    "*.csv": "gc-excelviewer-csv-editor",
    "*.md": "vscode.markdown.preview.editor",
    "*.xlsx": "gc-excelviewer-csv-editor"
  },
  "workbench.iconTheme": "vscode-great-icons",
  "workbench.startupEditor": "none",
  "zenMode.fullScreen": false,
  "[dotenv]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[ignore]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[hosts]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  }
}
